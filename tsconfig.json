{
  "compilerOptions": {
    "module": "es6",
    "target": "es5",
    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "allowJs": true,
    "moduleResolution": "node"
  },
  "exclude": ["node_modules"]
}
